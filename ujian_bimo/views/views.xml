<odoo>
  <data>
    <record id="view_barang_tree" model="ir.ui.view">
      <field name="name">ujian.barang.tree</field>
      <field name="model">ujian.barang</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="harga"/>
          <field name="stok"/>
        </tree>
      </field>
    </record>

    <record id="view_barang_form" model="ir.ui.view">
      <field name="name">ujian.barang.form</field>
      <field name="model">ujian.barang</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>
              <field name="harga"/>
              <field name="stok"/>
              <field name="keterangan"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="action_barang" model="ir.actions.act_window">
      <field name="name">Barang Gudang</field>
      <field name="res_model">ujian.barang</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_utama_ujian" name="Ujian Bimo"/>
    <menuitem id="menu_barang_bimo" name="Daftar Barang" parent="menu_utama_ujian" action="action_barang"/>
  </data>
</odoo>